project Magnetar_Ada is
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("magnetar-main.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-gnat2022", "-gnata", "-gnato", "-fstack-check", "-gnatwa");
   end Compiler;
end Magnetar_Ada;
